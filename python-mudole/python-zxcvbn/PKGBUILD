pkgname='python-zxcvbn'
_pkgname='zxcvbn-python'
pkgver=4.4.28
pkgrel=1
pkgdesc="Password strength estimator"
arch=('any')
url="https://github.com/dwolfhub/zxcvbn-python"
license=('Apache')
source=("https://github.com/dwolfhub/${_pkgname}/archive/v$pkgver.tar.gz")
sha256sums=('b7275765acdf8028c21aa502d742e56de2252bac604c04ba5e336c39f88d5576')
depends=('python')
makedepends=('python-setuptools')

package() {
  cd ${_pkgname}-$pkgver
  python setup.py install --root=${pkgdir}
}